
Node *firstNode(Node *head)
{
	//    Write your code here.
    if(head==NULL||head->next==NULL) return NULL;
    Node* slow=head;
    Node* fast=head;
    
    slow=slow->next;
    fast=fast->next->next;
    while(slow==fast){
        if(slow==fast) break;
        slow=slow->next;
        fast=fast->next->next;
    }
    if(slow!=fast) return NULL;
    
    slow=head;
    while(slow!=NULL){
        slow=slow->next;
        fast=fast>next;
    }
    return slow;
 
    
}
