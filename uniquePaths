#include <bits/stdc++.h> 
int countPath(int i,int j,int n, int m){
    if(i==(n-1) && j==(m-1)) return 1;
    if(i>=n || j>=m) return 0;
    else{
        return countPath(i+1,j,n,m)+countPath(i,j+1,n,m);
    }
}
int uniquePaths(int m, int n) {
	// Write your code here.
    return countPath(0,0,m,n);
}
